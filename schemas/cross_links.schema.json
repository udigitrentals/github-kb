{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "edges"
  ],
  "properties": {
    "edges": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "source",
          "target"
        ],
        "properties": {
          "source": {
            "type": "string"
          },
          "target": {
            "type": "string"
          },
          "rel": {
            "type": "string"
          }
        }
      }
    },
    "updated_at": {
      "type": "string"
    }
  }
}